{
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "target": true,
        ".vscode": true,
        "Cargo.lock": true,
        "dist": true
    },
    "hide-files.files": [
        "target",
        ".vscode",
        "Cargo.lock",
        "dist"
    ],
    "[rust]": {
        "editor.defaultFormatter": "rust-lang.rust-analyzer",
        "editor.formatOnSave": true
    }
}