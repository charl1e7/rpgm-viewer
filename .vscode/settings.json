{
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        ".vscode": true,
        "target": true,
        "rpgm-viewer/dist": true,
        "rpgm-viewer/Cargo.lock": true,
        "rpgm-viewer/.vscode": true,
        "rpgm-enc/.vscode": true,
        "rpgm-enc/Cargo.lock": true
    },
    "hide-files.files": [
        ".vscode",
        "target",
        "rpgm-viewer/dist",
        "rpgm-viewer/Cargo.lock",
        "rpgm-viewer/.vscode",
        "rpgm-enc/.vscode",
        "rpgm-enc/Cargo.lock"
    ],
    "[rust]": {
        "editor.defaultFormatter": "rust-lang.rust-analyzer",
        "editor.formatOnSave": true
    }
}